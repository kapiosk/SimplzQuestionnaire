{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=wwwroot\\AppData\\SQ.db"
  },
  "Antiforgery": "caketoken",
  "JWTSettings": {
    "SecretKey": "caketoken"
  },
  "CookieSettings": {
    "LoginPath": "/Authorization/Login"
  },
  "IdentityCoreSettings": {
    "Password": {
      "RequireDigit": false,
      "RequireLowercase": false,
      "RequireUppercase": false,
      "RequireNonAlphanumeric": false,
      "RequiredLength": 1
    },
    "User": {
      "RequireUniqueEmail": false
    },
    "SignIn": {
      "RequireConfirmedEmail": false
    }
  }
}