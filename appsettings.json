{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=wwwroot\\AppData\\SQ.db"
  },
  "Antiforgery": "caketoken",
  "JWTSettings": {
    "SecretKey": "caketoken"
  },
  "CookieSettings": {
    "LoginPath": "/Authorization/Login",
    "SlidingExpiration": true,
    "Cookie": {
      "HttpOnly": true
    }
  },
  "IdentityCoreSettings": {
    "Password": {
      "RequireDigit": false,
      "RequireLowercase": false,
      "RequireUppercase": false,
      "RequireNonAlphanumeric": false,
      "RequiredLength": 1
    },
    "User": {
      "RequireUniqueEmail": false
    },
    "SignIn": {
      "RequireConfirmedEmail": false
    }
  },
  "QRURL": "https://simplzquestionnaire.azurewebsites.net/API/QR/", //https://simplzquestionnaire.azurewebsites.net
  "QURL": "https://simplzquestionnaire.azurewebsites.net/Questionnaires/Active/" //https://localhost:44385
}